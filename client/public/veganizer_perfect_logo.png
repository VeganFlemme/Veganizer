<svg xmlns="http://www.w3.org/2000/svg" width="1024" height="1024" viewBox="0 0 1024 1024">
  <title>Veganizer - Logo Parfait - Transformation Végétale</title>
  <defs>
    <!-- Gradient principal pour la transformation -->
    <linearGradient id="transformGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#00C851;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#4CAF50;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#2E7D32;stop-opacity:1" />
    </linearGradient>
    
    <!-- Gradient pour la feuille -->
    <linearGradient id="leafGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#81C784;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#00C851;stop-opacity:1" />
    </linearGradient>
    
    <!-- Gradient pour l'arrière-plan moderne -->
    <radialGradient id="bgGradient" cx="50%" cy="50%" r="50%">
      <stop offset="0%" style="stop-color:#ffffff;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#f8f9fa;stop-opacity:1" />
    </radialGradient>
    
    <!-- Ombre douce -->
    <filter id="softShadow" x="-50%" y="-50%" width="200%" height="200%">
      <feDropShadow dx="8" dy="8" stdDeviation="12" flood-color="#2E7D32" flood-opacity="0.2"/>
    </filter>
  </defs>
  
  <!-- Arrière-plan moderne -->
  <rect width="1024" height="1024" rx="160" ry="160" fill="url(#bgGradient)" stroke="#e8f5e8" stroke-width="8"/>
  
  <!-- Conteneur principal centré -->
  <g transform="translate(512,512)">
    
    <!-- Flèche de transformation circulaire principale -->
    <g filter="url(#softShadow)">
      <!-- Cercle extérieur de transformation -->
      <path d="M -200 -80 A 200 200 0 1 1 -200 80" 
            fill="none" 
            stroke="url(#transformGradient)" 
            stroke-width="32" 
            stroke-linecap="round"/>
      
      <!-- Pointe de flèche moderne -->
      <path d="M -180 -120 L -120 -80 L -180 -40 Z" 
            fill="url(#transformGradient)"/>
    </g>
    
    <!-- Feuille stylisée intégrée -->
    <g transform="rotate(15)">
      <!-- Corps de la feuille -->
      <path d="M 40 -120 
               Q 120 -140, 160 -60
               Q 180 0, 160 60
               Q 120 140, 40 120
               Q 60 80, 80 40
               Q 100 0, 80 -40
               Q 60 -80, 40 -120 Z" 
            fill="url(#leafGradient)" 
            filter="url(#softShadow)"/>
      
      <!-- Nervure centrale de la feuille -->
      <path d="M 40 -120 Q 90 -20, 40 120" 
            fill="none" 
            stroke="#2E7D32" 
            stroke-width="6" 
            stroke-linecap="round"/>
      
      <!-- Nervures secondaires -->
      <path d="M 60 -60 Q 100 -40, 120 -20" 
            fill="none" 
            stroke="#2E7D32" 
            stroke-width="4" 
            stroke-linecap="round" 
            opacity="0.7"/>
      <path d="M 70 0 Q 110 20, 130 40" 
            fill="none" 
            stroke="#2E7D32" 
            stroke-width="4" 
            stroke-linecap="round" 
            opacity="0.7"/>
      <path d="M 60 60 Q 100 80, 120 100" 
            fill="none" 
            stroke="#2E7D32" 
            stroke-width="4" 
            stroke-linecap="round" 
            opacity="0.7"/>
    </g>
    
    <!-- Élément de connexion/transformation -->
    <circle cx="0" cy="0" r="24" fill="url(#transformGradient)" filter="url(#softShadow)"/>
    <circle cx="0" cy="0" r="16" fill="#ffffff" opacity="0.9"/>
    <circle cx="0" cy="0" r="8" fill="url(#transformGradient)"/>
    
    <!-- Points de transformation (effet moderne) -->
    <g opacity="0.6">
      <circle cx="-140" cy="-140" r="6" fill="#00C851"/>
      <circle cx="140" cy="-140" r="6" fill="#4CAF50"/>
      <circle cx="140" cy="140" r="6" fill="#2E7D32"/>
      <circle cx="-140" cy="140" r="6" fill="#81C784"/>
    </g>
    
    <!-- Effet de brillance moderne -->
    <g opacity="0.3">
      <ellipse cx="-80" cy="-120" rx="20" ry="40" fill="#ffffff" transform="rotate(-30)"/>
    </g>
    
  </g>
  
  <!-- Texte Veganizer (version complète) -->
  <g transform="translate(512, 820)">
    <text x="0" y="0" 
          font-family="'SF Pro Display', 'Helvetica Neue', Arial, sans-serif" 
          font-size="64" 
          font-weight="600" 
          text-anchor="middle" 
          fill="url(#transformGradient)"
          letter-spacing="2px">
      Veganizer
    </text>
  </g>
  
</svg>